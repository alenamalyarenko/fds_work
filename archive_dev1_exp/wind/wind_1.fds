&HEAD CHID='Wind_1'/

# GRID

# size of each mesh, edge to edge 
# 9 cpu
&MESH IJK=20,20,60, XB=0.0,320.0,  0.0,320.0,  0,540,  MULT_ID='mesh_fire' /
&MULT ID='mesh_fire', DX=320, DY=320, DZ=540., I_UPPER=2, J_UPPER=2, K_UPPER=0/



&TIME T_END=600, LOCK_TIME_STEP=F, DT=1 /

&WIND SPEED=4.8, DIRECTION=270., L=-500., Z_0=0.03 /


#roof - always open - HOT
&VENT MB='ZMAX', SURF_ID='OPEN'/

#west - coupled
&VENT XB=0.,0.,     0.,960., 0.,540.,SURF_ID='OPEN'/
#east - coupled
&VENT XB=960.,960., 0.,960., 0.,540.,SURF_ID='OPEN'/


#south - coupled
&VENT XB=0.,960.,     0.,0., 0.,540.,SURF_ID='OPEN'/
#north - coupled
&VENT XB=0.,960., 960.,960., 0.,540.,SURF_ID='OPEN'/


#&VENT XB=0.,960.,0.,960.,0.,0., SURF_ID='surface' /
#&SURF ID='surface',   CONVECTIVE_HEAT_FLUX=0.36 / # kW/m2

&MISC SIMULATION_MODE='SVLES', LEVEL_SET_MODE=4 //
&MISC VERBOSE=T/

# Outputs:
&DUMP DT_PL3D=25, DT_HRR =0, SIG_FIGS=4, SIG_FIGS_EXP=2 , WRITE_XYZ=T /
&ISOF QUANTITY='TEMPERATURE', VALUE(1:4)=30.0, 31.0, 32., 33 /


#Crown Fire
&REAC FUEL='FUEL VAPOR', C=3.4, H=6.2, O=2.5, SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=17700. /
&SURF ID='FIRE', HRRPUA=1250, COLOR='RED' RAMP_Q='HRR' /
&VENT XB= 160,240,80,880,0,0, SURF_ID='FIRE' /
&RAMP ID='HRR', T=0.0, F=0.0 /
&RAMP ID='HRR', T=3, F=1.0 /
&RAMP ID='HRR', T=300, F=1.0 /



&TAIL /
